generator client {
  provider = "prisma-client-js"
  output   = "./../generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("FCN_DATABASE_URL")
}

model User {
  id         String   @id @default(cuid())
  email      String?  @unique
  name       String?
  firstLogin DateTime @default(now())
  lastLogin  DateTime @default(now())
  loginCount Int      @default(1)
}
